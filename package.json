    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "hbs": "^4.1.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mathjs": "^9.2.0",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.4",
